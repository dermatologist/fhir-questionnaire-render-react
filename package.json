{
  "name": "fhir-questionnaire-render-react",
  "description": "Rendering engine for FHIR questionnaires using react and FHIRFormJS",
  "version": "4.0.0",
  "author": "Bell Eapen",
  "private": true,
  "dependencies": {
    "@rjsf/core": "^2.5.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.9",
    "axios": "^0.21.1",
    "fhirformjs": "^0.8.1",
    "jsoneditor": "^9.4.1",
    "jsoneditor-react": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(jsoneditor-react/))"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "identity-obj-proxy": "^3.0.0",
    "react-test-renderer": "^17.0.2"
  }
}
